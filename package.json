{
  "name": "testing-react-apps",
  "version": "0.0.1",
  "description": "Testing React apps with React Testing Library",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./tests/. ./src/. --ext js --ext jsx --fix",
    "test": "jest --testTimeout=7000",
    "test:watch": "jest --watch --testTimeout=7000",
    "start": "webpack-dev-server --mode development --devtool source-map --hot --open",
    "build": "webpack --mode production"
  },
  "keywords": [
    "react",
    "webpack",
    "testing",
    "rtl"
  ],
  "author": "ritamcastro",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.2.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
